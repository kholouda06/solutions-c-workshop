#include <stdio.h>

int main() {
    float a, b;

    printf("Entrez le premier nombre : ");
    scanf("%f", &a);
    printf("Entrez le deuxième nombre : ");
    scanf("%f", &b);
    printf("a + b = %.2f\n", a + b);
    printf("a - b = %.2f\n", a - b);
    printf("a * b = %.2f\n", a * b);
    
    if (b != 0) {
        printf("a / b = %.2f\n", a / b);
    } else {
        printf("La division par zéro est impossible.\n");
    }

    return 0;
}